{% extends 'base.html.twig' %}

{% block title %}Page d'accueil{% endblock %}

{% block body %}
    <div class="card-deck">
{% for membre in membres %}
    <div class="card col-md-6" style="min-width: 160px">
        <img class="card-img-top" src="{{ membre.image }}" alt="Card image cap">
        <div class="card-body">
            <h3 class="card-title">{{ membre.pseudo }}</h3>
            <p class="card-text">{{ membre.description }}</p>
        </div>
    </div>
{% endfor %}
    </div>
{% endblock %}
